{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "bas-adf"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1'"
		},
		"Sftp2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp2'"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://staging1bas.blob.core.windows.net/"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "sftpfolder1.blob.core.windows.net"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sftpfolder1.bas14"
		},
		"Sftp2_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "sftpfolder1.blob.core.windows.net"
		},
		"Sftp2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sftpfolder1.sftp.bas14"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bas-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"Sftp2_password": {
						"value": "[parameters('Sftp2_password')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					},
					"Sftp2_properties_typeProperties_host": {
						"value": "[parameters('Sftp2_properties_typeProperties_host')]"
					},
					"Sftp2_properties_typeProperties_userName": {
						"value": "[parameters('Sftp2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}